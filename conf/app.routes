# app.routes

# routes to specific services

POST     /register/movie                             controllers.MovieTicketReservationControllerImpl.registerMovie
POST     /reserve/seat                               controllers.MovieTicketReservationControllerImpl.reserveSeat
GET      /movie/information/:imdbId/:screenId        controllers.MovieTicketReservationControllerImpl.viewMovieBookingInfo(imdbId: String, screenId: String)


# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
